{
  "name": "harvest-universal-liquidator-arbitrum",
  "version": "1.0.0",
  "description": "This is a liquidation router that efficiently liquidates assets across multiple DEXes, including Uniswap, Sushiswap, Balancer, etc.",
  "homepage": "https://github.com/harvestfi/harvest-universal-liquidator-arbitrum",
  "license": "MIT",
  "main": "hardhat.config.ts",
  "scripts": {
    "compile": "hardhat compile",
    "test": "hardhat test"
  },
  "author": "",
  "devDependencies": {
    "@ethersproject/abi": "^5.4.7",
    "@ethersproject/providers": "^5.4.7",
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.0",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.0",
    "@nomicfoundation/hardhat-toolbox": "^2.0.0",
    "@nomiclabs/hardhat-ethers": "2.2.3",
    "@nomiclabs/hardhat-etherscan": "3.1.7",
    "@openzeppelin/contracts": "4.9.6",
    "@openzeppelin/contracts-upgradeable": "4.3.3",
    "@typechain/ethers-v5": "^10.1.0",
    "@typechain/hardhat": "^6.1.2",
    "@types/chai": "^4.2.0",
    "@types/mocha": ">=9.1.0",
    "@types/node": ">=12.0.0",
    "@types/prompts": "^2.4.4",
    "@uniswap/v2-core": "^1.0.1",
    "@uniswap/v2-periphery": "^1.1.0-beta.0",
    "chai": "^4.2.0",
    "dotenv": "^10.0.0",
    "ethers": "^5.4.7",
    "hardhat": "2.22.17",
    "hardhat-gas-reporter": "^1.0.8",
    "prompt": "1.2.0",
    "prompts": "^2.4.2",
    "solidity-coverage": "^0.8.0",
    "ts-node": ">=8.0.0",
    "typechain": "^8.1.0",
    "typescript": ">=4.5.0"
  },
  "config": {
    "solidity": {
      "compilers": [
        {
          "version": "0.8.17",
          "settings": {
            "optimizer": {
              "enabled": true,
              "runs": 3000
            }
          }
        }
      ]
    },
    "paths": {
      "contracts": "./contracts",
      "tests": "./tests",
      "cache": "./cache",
      "build": {
        "contracts": "./build/contracts",
        "typechain": "./build/typechain"
      }
    }
  }
}
